{
  "manifest_version": 2,

  "name": "Account Switcher",
  "version": "1.0",

  "permissions": [
    "http://translate.yandex.net/*",
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage",
    "https://github.com/"
  ],

  "background": {
    "scripts": ["popup.js"]
  },

  "browser_action": {
    "default_title": "Open",
    "default_icon": "icons/default.jpg",
    "default_popup": "popup.html"
  }
}
